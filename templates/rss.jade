doctype xml
rss(version='2.0')
  channel
    title= channel.title
    description= channel.description
    language en-us
    link= channel.link 
    lastBuildDate= channel.date
    pubDate= channel.date

    each item in items
      item
        title= item.title
        description= item.content
        link= item.link
        pubDate= item.date
